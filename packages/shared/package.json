    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "regenerator-runtime": "0.11.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.1.0",
    "standard-version": "^9.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0",
