    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "cp CHANGELOG.md docs/introduce/ && git add docs/introduce/CHANGELOG.md"
    }
  }
}
